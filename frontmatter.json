{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.website.host": "https://alan.norbauer.com",
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.": [],
  "frontMatter.content.pageFolders": [
    {
      "title": "Articles",
      "path": "[[workspace]]/src/content/articles"
    }
  ],
  "fontMatter.content.publicFolder": {
    "path": "public",
    "relative": true
  },
  "fontMatter.content.draftField": {
    "name": "draft",
    "type": "boolean"
  },
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "fileType": "mdx",
      "defaultFileName": "index",
      "pageBundle": true,
      "previewPath": "'articles",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "author",
          "name": "author",
          "type": "string",
          "required": false
        },
        {
          "title": "date_created",
          "name": "date_created",
          "type": "datetime",
          "isPublishDate": true,
          "required": true
        },
        {
          "title": "date_updated",
          "name": "date_updated",
          "type": "datetime",
          "isModifiedDate": true,
          "required": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string",
          "required": true
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime",
          "default": false
        },
        {
          "title": "series",
          "name": "series",
          "type": "string",
          "required": false
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags",
          "default": []
        },
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "required": true
        }
      ]
    }
  ]
}
